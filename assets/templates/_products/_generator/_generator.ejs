<!-- Modal -->
<div class="modal fade" id="generator" tabindex="-1" role="dialog" aria-labelledby="productModalLabel"
	aria-hidden="true">
	<div class="modal-dialog modal-lg" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="productModalLabel">Generator</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body">
				<div class="row">
					<div class="col-md-6">
						<% include _carousel %>
					</div>
					<div class="col-md-6">
						<!-- <h2>USD 1111</h2> -->
						<span class="badge badge-primary text-uppercase">Energy</span>
						<p class="mt-2">
							Generators supply continuous electrical power (at variable load) in lieu of commercially
							purchased power. There is no limitation to the annual hours of operation and the models
							provided by UNHRD can supply 10% overload power for 1 hour in 12 hours.
						</p>
						<div class="row justify-content-between align-items-center mb-3">
							<div class="col">
								<label for="" class="d-block">Type</label>
								<select class="custom-select custom-select-outline">
									<option selected>Diesel, 45KVA, w/spares & trailer </option>
									<option value="1">GENERATOR, DIESEL, 16 KVA, C</option>
									<option value="1">DIESEL, 18 KVA, FG WILSON</option>
								</select>
							</div>
							<div class="col">
								<label for="">Quantity</label>
								<input class="form-control form-control-outline w-50" type="number" value="1">
							</div>
						</div>

						<a href="#" class="btn btn-block btn-primary">
							<svg class="navbar-icon">
								<use xlink:href="sprite.svg#star"></use>
							</svg>
							<span>Add to wishlist</span>
							<img src="images/chevron-right.svg" alt="Arrow right">
						</a>

						<a href="#" class="btn btn-block btn-secondary">
							<svg class="navbar-icon">
								<use xlink:href="sprite.svg#download"></use>
							</svg>
							<span>Technical Details</span>
							<img src="images/chevron-right.svg" alt="Arrow right">
						</a>
					</div>
				</div>
			</div>
			<div class="modal-footer">
				<h5 class="d-flex align-items-center">
					<svg class="navbar-icon mr-2">
						<use xlink:href="sprite.svg#box"></use>
					</svg>
					Accessories
				</h5>
			</div>
		</div>
	</div>
</div>