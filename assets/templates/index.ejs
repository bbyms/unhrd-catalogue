                    <img src="images/chevron-right.svg" alt="Arrow right">
                  </a>
                </div>
              <div class="col-md-6">
                <a href="services.html" class="btn btn-block btn-navy">
                    <svg class="navbar-icon">
                        <use xlink:href="sprite.svg#list"></use>
                    </svg>
